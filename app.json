{
    "name": "EXviewer",
    "description": "PWA for the Exhentai",
    "keywords": ["pwa", "exhentai"],
    "env": {
        "EH_COOKIE": {
            "description": "E站的cookie值",
            "value": "ipb_member_id=******; ipb_pass_hash=******; igneous=******; sl=dm_1; sk=******; s=******"
        },
        "EH_FAVORITE_DISABLED": {
            "description": "禁用添加删除收藏",
            "value": "true"
        },
        "EH_DOWNLOAD_DISABLED": {
            "description": "禁用下载功能",
            "value": "true"
        },
        "EH_COMMENT_DISABLED": {
            "description": "禁用评论功能",
            "value": "true"
        },
        "EH_RATE_DISABLED":{
            "description": "禁用评分功能",
            "value": "true"
        },
        "UTC_OFFSET":{
            "description": "UTC时间偏移量",
            "value": "+8"
        }
    },
    "website": "https://github.com/DriverLin/EXviewer",
    "repository": "https://github.com/DriverLin/EXviewer",
    "success_url": "/"
}